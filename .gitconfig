[user]
    name = daigo
    email = dragonmind2002@yahoo.co.jp
[core]
    whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
    editor=vim
[color]
    ui = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
    st = status
    ad = add
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --date=short --pretty='format:%C(black bold)%ad %C(yellow)%h%Cblue%d%Creset %s %C(black bold)%an%Creset' --abbrev-commit --all
    lolas = log --graph --decorate --date=short --pretty='format:%C(black bold)%ad %C(yellow)%h%Cblue%d%Creset %s %C(black bold)%an%Creset' --abbrev-commit --all --stat
    ls = ls-files
